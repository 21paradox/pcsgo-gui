{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "paths": {
        "*": ["./src/*"]
     }
  },
   "include": [
        "src/*"
   ],
}
